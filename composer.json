{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.4.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*@dev",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "symfony/monolog-bundle": "2.2.*",
        "sensio/distribution-bundle": "2.2.*",
        "sensio/framework-extra-bundle": "dev-master",
        "sensio/generator-bundle": "2.2.*",
        "jms/security-extra-bundle": "1.5.*@dev",
        "jms/di-extra-bundle": "1.4.*@dev",
        "winzou/console-bundle": "1.*",
        "stof/doctrine-extensions-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/data-fixtures": "@dev",
        "friendsofsymfony/user-bundle": "dev-master",
        "knplabs/knp-markdown-bundle": "1.2.*@dev",
        "ornicar/akismet-bundle": "dev-master",
        "willdurand/expose-translation-bundle": ">=0.2.1",
        "apy/jsfv-bundle":"dev-master",
        "gedmo/doctrine-extensions":      "dev-master",
        "codeconsortium/ccdn-forum-bundle": "dev-master",
        "creof/doctrine2-spatial": "dev-master",
        "twbs/bootstrap": "v3.0.0"

    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
