<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
    
    <link rel="shortcut icon" href="{{ asset('icon/favicon.png') }}" /> 

    <title>{% block title %}Extendscript-library{% endblock %}</title>

    <!-- Bootstrap core CSS -->
    {% block stylesheets %}
      {% stylesheets output='css/main.css' filter='cssrewrite, ?yui_css'
        'css/bootstrap.css'
        'css/theme.css'
      %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
      {% endstylesheets %}
    {% endblock %}
        

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="{{ asset('js/html5shiv.js') }}"></script>
      <script src="{{ asset('js/respond.min.js') }}"></script>
    <![endif]-->

    
  </head>
  {# Onload event is used one the page start to prettify code #}
  <body {% if onLoadEvent is defined %} {{ onLoadEvent }} {% endif %}>
    {# Not display navbar on login page #}
    {% if app.request.attributes.get('_route') != 'fos_user_security_login' %}
          {% include 'nav.html.twig' %}
    {% endif %}
    
    
    {# Block carousel is only on the home page #}
    {% if app.request.attributes.get('_route') == 'jlfa_web_site_homepage' 
        or app.request.attributes.get('_route') == 'root' %}
          {% block carousel %}
          {% endblock %}
    {% endif %}
    

    <!-- start content -->
    <div class="container marketing first">
        {% block content %}
        {% endblock %}
        
      <hr class="featurette-divider">
      <!-- FOOTER -->
      {% if app.request.attributes.get('_route') != 'fos_user_security_login' %}
        <footer>
          <p class="pull-right"><a href="#">Back to top</a></p>
          <p>&copy; 2013 Company, Inc. &middot; <a href="#">Privacy</a> &middot; <a href="#">Terms</a></p>
        </footer>
      {% endif %}
    </div><!-- end content -->
    {% block javascripts %}
      {% javascripts output='js/main.js' filter='?yui_js'
        'js/jquery.min.js'
        'js/bootstrap.min.js'
        'js/holder.js'
        'js/nav.js'
      %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
    {% endblock %}
  </body>
</html>
